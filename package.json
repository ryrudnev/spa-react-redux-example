  "scripts": {
    "start": "yarn run clean:dist && cross-env NODE_ENV=development nodemon ./bin/server.js",
    "start:debug": "yarn run clean:dist && cross-env NODE_ENV=development cross-env DEBUG=ncs:* nodemon ./bin/server.js",
    "start:prod": "cross-env NODE_ENV=production node ./bin/server.js",
    "start-ssr": "yarn run clean:dist && cross-env NODE_ENV=development concurrently --kill-others \"nodemon ./bin/server.js --ssr\" \"node ./bin/dev-server-ssr.js --ssr\"",
    "start-ssr:debug": "yarn run clean:dist && cross-env NODE_ENV=development cross-env DEBUG=ncs:* concurrently --kill-others \"nodemon ./bin/server.js --ssr\" \"node ./bin/dev-server-ssr.js --ssr\"",
    "start-ssr:prod": "cross-env NODE_ENV=production node ./bin/server.js --ssr",
    "clean:dist": "rimraf ./public/dist",
    "clean:cache": "rimraf ./.cache",
    "lint:css": "./node_modules/.bin/stylelint \"./src/**/*.css\"",
    "lint:js": "./node_modules/.bin/eslint --config .eslintrc ./src ./config ./bin",
    "lint": "concurrently --kill-others \"yarn run lint:js\" \"yarn run lint:css\"",
    "build": "yarn run clean:dist && cross-env NODE_ENV=production webpack --progress --verbose --display-error-details --config ./config/webpack.config.js",
    "build:ssr": "yarn run build -- --env.ssr"
  }
